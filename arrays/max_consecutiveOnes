class Solution {
    public int findMaxConsecutiveOnes(int[] nums) {
        int n=nums.length;
        int start=0;
        int end=n-1;
        int max=Integer.MIN_VALUE;
        for(int i=0;i<n;i++){
            if(nums[i]!=1){
              
                max=Math.max(max,i-start);
                start=i+1;
                
                
            }
            
            
            



        }
        max=Math.max(max,(n-start));
        return max;
    }
}